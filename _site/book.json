{
  "root": "book",
  "title": "IBM Cloud Kubernetes Service Lab",
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "fancybox": {
      "openEffect": "elastic",
      "closeEffect": "elastic"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ],
      "prism": {
        "lang": {
          "flow": "typescript"
        }
      },
      "ignore": [
        "mermaid", 
        "eval-js" 
      ],
      "hints": {
        "info": "fa fa-info-circle",
        "tip": "fa fa-mortar-board",
        "danger": "fa fa-exclamation-cicle",
        "working": "fa fa-wrench"
      }
    },
    "webheader": {
      "headerTitle" : "IBM Cloud Kubernetes Service (IKS) Lab"
    }
  },
  "plugins": [
    "prism", "-highlight",
    "collapsible-chapters",
    "hints",
    "copy-code-button",
    "back-to-top-button",
    "fancybox",
    "web-header"
  ],
  "gitbook": "3.2.3"
}
